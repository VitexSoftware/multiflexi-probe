#!/bin/sh
# postinst script for multiflexi-probe
set -e

. /usr/share/debconf/confmodule

APPLICATION=multiflexi-probe
for file in `ls /usr/lib/$APPLICATION/multiflexi/*.app.json`
do
	multiflexi-json2app $file
done

for file in `ls /usr/lib/$APPLICATION/multiflexi/*.credential-type.json`
do
	multiflexi-json2credential-type $file
done
